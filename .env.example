# Database (Railway PostgreSQL)
DATABASE_URL=postgresql://user:password@host:port/dbname

# Redis (Railway Redis)
REDIS_URL=redis://default:password@host:port

# NextAuth
NEXTAUTH_SECRET=generate-with-openssl-rand-base64-32
